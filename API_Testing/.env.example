# FirstMile Tracking API Configuration
# Copy this file to .env and fill in your credentials

# JWT Token from FirstMile (required for authentication)
FIRSTMILE_JWT_TOKEN=your_jwt_token_here

# Optional: TechPartnerId if provided by FirstMile
FIRSTMILE_TECH_PARTNER_ID=

# Default environment (test, production, or mock)
FIRSTMILE_ENVIRONMENT=test
